rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are permissive and are intended for development purposes only.
    // They allow any authenticated user to read and write any data.
    // You MUST secure these rules before deploying your app to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
