rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are completely insecure and for development purposes only.
    // They allow anyone to read, write, and delete any data in your database.
    // You MUST secure these rules before deploying your app to production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
