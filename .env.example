
# Rename this file to .env.local and fill in your values
# Get these from your Razorpay Dashboard: https://dashboard.razorpay.com/app/keys

# For server-side use (API routes)
RAZORPAY_KEY_ID=YOUR_KEY_ID
RAZORPAY_KEY_SECRET=YOUR_KEY_SECRET

# For client-side use (Must have NEXT_PUBLIC_ prefix)
NEXT_PUBLIC_RAZORPAY_KEY_ID=YOUR_KEY_ID
